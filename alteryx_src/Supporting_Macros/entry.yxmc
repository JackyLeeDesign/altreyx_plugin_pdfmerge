          <productionModeScript>#################################
# List all non-standard packages to be imported by your 
# script here (only missing packages will be installed)
from ayx import Package

Package.installPackages(package=['pandas','PyPDF2'], install_type="install --user")


#################################
from ayx import Alteryx
import pandas as pd
from PyPDF2 import PdfMerger
import os
data = Alteryx.read("#3")
output_file = Alteryx.read("#2")
file_list = data['file_list']
output_file = output_file['output_file'].iloc[0]
resultTemplate = {
    "FilePath": file_list
}
resultData = pd.DataFrame(resultTemplate)
resultData["Status"] = "Success"
try:
    # 獲取所有pdf檔案完整路徑
#     def getAllpdfFiles(filedir):
#         file_list = [os.path.join(root, filespath) \
#                     for root, dirs, files in os.walk(filedir) \
#                     for filespath in files \
#                     if str(filespath).endswith('pdf')
#                     ]
#         return file_list if file_list else []

    merger = PdfMerger()
    
    resultTemplate = {
        "name": ["FilePath", "Status", "Message"],
        "FilePath": file_list
    }
    resultData = pd.DataFrame(resultTemplate)
#     pdf_file_list = getAllpdfFiles(filedir)
    for i in range(len(file_list)):
        errorMessage = ""
        try:
            isExist = os.path.exists(file_list[i])
            if(isExist== True):
                merger.append(open(file_list[i],"rb"))
            else:
                errorMessage = "該檔案不存在! " + file_list[i]
        except Exception as e:
            errorMessage =  str(e)
            
        if (not errorMessage):
            resultData.at[i, "Status"] = "Success"
            resultData.at[i, "Message"] = ""
        else:
            resultData.at[i, "Status"] = "Failure"
            resultData.at[i, "Message"] = errorMessage

    # Write to an output PDF document
    output = open(os.path.join(output_file,"merge_result.pdf"),"wb")
    merger.write(output)
    result = os.path.join(output_file,"merge_result.pdf")
#     data["status"] = "Success：" + result
    print("Success")
except Exception as e:
#     data["status"] = "Failure：" + str(e)
    print("Failure：" + str(e))
Alteryx.write(resultData,1)


#################################



#################################
</productionModeScript>
